{
  "name": "portfolio",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro check && astro build",
    "build:node": "astro check && astro build --node",
    "preview": "astro preview",
    "preview:node": "astro preview --node",
    "astro": "astro",
    "lint": "prettier --write  \"**/*.{js,jsx,ts,tsx,md,mdx,astro}\" && eslint --fix \"src/**/*.{js,ts,jsx,tsx,astro}\" && echo \"\\e[0;32m✔\\e[0;0m Linted files\"",
    "sanity:typegen": "rm -f sanity/schema/schema.json && echo \"\\e[0;32m✔\\e[0;0m Deleted old schema.json\" && rm -f src/types/sanity.ts && echo \"\\e[0;32m✔\\e[0;0m Deleted old type definition\" && sanity schema extract --path sanity/schema/schema.json --enforce-required-fields && sanity typegen generate",
    "prepare": "husky"
  },
  "dependencies": {
    "@astrojs/check": "^0.9.4",
    "@astrojs/node": "^9.1.2",
    "@astrojs/react": "^4.2.1",
    "@astrojs/sitemap": "^3.2.1",
    "@astrojs/vercel": "^8.1.1",
    "@fontsource-variable/inter": "^5.2.5",
    "@sanity/astro": "^3.2.6",
    "@sanity/client": "^6.28.3",
    "@sanity/code-input": "^5.1.2",
    "@sanity/vision": "^3.78.1",
    "@shikijs/transformers": "^1.29.2",
    "@tailwindcss/vite": "^4.0.12",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@vercel/analytics": "^1.5.0",
    "@vercel/speed-insights": "^1.2.0",
    "astro": "^5.4.2",
    "astro-portabletext": "^0.11.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "groq": "^3.78.1",
    "katex": "^0.16.21",
    "medium-zoom": "^1.1.0",
    "prism-react-renderer": "^2.4.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "sanity": "^3.78.1",
    "sanity-plugin-dashboard-widget-vercel": "^3.1.3",
    "sanity-plugin-media": "^3.0.1",
    "tailwind-merge": "^3.0.2",
    "tailwindcss": "^4.0.12",
    "typescript": "^5.4.2"
  },
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/katex": "^0.16.7",
    "@typescript-eslint/parser": "^7.3.1",
    "eslint": "^8.57.0",
    "eslint-plugin-astro": "^0.31.4",
    "husky": "^9.0.11",
    "prettier": "^3.2.5",
    "prettier-config-standard": "^7.0.0",
    "prettier-plugin-astro": "^0.13.0",
    "prettier-plugin-tailwindcss": "^0.6.11"
  }
}
